{
  "name": "thundra-foresight-python-action",
  "version": "0.0.1",
  "description": "Thundra Foresight Python Action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm install && npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thundra-io/thundra-foresight-python-action.git"
  },
  "keywords": [],
