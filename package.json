  "bugs": {
    "url": "https://github.com/thundra-io/thundra-foresight-python-action/issues"
  },
  "homepage": "https://github.com/thundra-io/thundra-foresight-python-action#readme",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^5.0.0",
    "@actions/io": "^1.1.0",
    "@actions/tool-cache": "^1.6.1",
    "@types/minimist": "^1.2.2",
    "@types/semver": "^7.3.5",
    "@types/shell-quote": "^1.7.1",
    "minimist": "^1.2.5",
    "semver": "^7.3.5",
